{
  "name": "@bcpros/localecash-testing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:interval": "node tests/scripts/run-tests-interval.js",
    "test:auth": "bash scripts/generate-auth-roles.sh",
    "test:auth:local": "bash scripts/generate-auth-roles.sh local",
    "test:auth:dev": "bash scripts/generate-auth-roles.sh dev",
    "test:auth:seller": "bash scripts/generate-auth-roles.sh Seller",
    "test:auth:buyer": "bash scripts/generate-auth-roles.sh Buyer",
    "test:auth:arb": "bash scripts/generate-auth-roles.sh Arb",
    "test:auth:dev:seller": "bash scripts/generate-auth-roles.sh dev Seller",
    "test:auth:dev:buyer": "bash scripts/generate-auth-roles.sh dev Buyer",
    "test:create-offer": "playwright test tests/create-offer.spec.ts --project=chromium --headed",
    "test:release": "playwright test tests/local/seller-release-local.spec.ts --project=chromium --headed",
    "test:release-with-deposit": "playwright test tests/local/seller-release-have-buyer-deposit-local.spec.ts --project=chromium --headed",
    "test:return": "playwright test tests/local/return-flow-local.spec.ts --project=chromium --headed",
    "test:return-with-deposit": "playwright test tests/local/return-flow-buyer-deposit-local.spec.ts --project=chromium --headed",
    "test:dispute": "playwright test tests/local/dispute-flow-local.spec.ts --project=chromium --headed",
    "test:dispute-with-deposit": "playwright test tests/local/dispute-flow-buyer-deposit-local.spec.ts --project=chromium --headed",
    "test:main-flows": "playwright test tests/local/ --project=chromium",
    "test:dev:random": "bash scripts/run-random-dev-test.sh",
    "test:codegen": "playwright codegen"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "dotenv": "^16.5.0",
    "eslint": "^8.56.0",
    "typescript": "^5"
  },
  "packageManager": "pnpm@7.0.0",
  "resolutions": {}
}